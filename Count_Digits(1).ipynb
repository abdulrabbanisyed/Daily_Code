   "source": [
    "def count_digits_in_number(n):\n",
    "    count = 0\n",
    "    while n != 0:\n",
    "        temp1 = n//10\n",
    "        n = temp1\n",
    "        count += 1\n",
    "    return count\n",
    "\n",
    "#Example\n",
    "count_digits_in_number(1234)"
   ]
