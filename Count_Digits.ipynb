    "def count_digits_in_number(n):\n",
    "    input_string = str(n)\n",
    "    count = 0\n",
    "    for number in input_string:\n",
    "        count += 1\n",
    "    return count\n",
    "\n",
    "#Example\n",
    "count_digits_in_number(1234)"
   ]
